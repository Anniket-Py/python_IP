This is a very basic bank managment system which interacts with Mysql Database using Mysql Connector. 

SQL QUERIES
create database Bank_mgt:
use bank_mgt;
create tale customer_details(customer_id int not null, customer_name varchar(30) not null, mobile_no int not null, dob date not null, account_id int not null);
create table account_details(account_id int, FOREIGN KEY(account_id_ REFRENCES customer_details(account_id),account_type varchar(20) not null, branch_id int not null);
create table employee_details(employee_id int not null, name varchar(30) not null, position varchar(30) notn null, department varchar(20) not null);
